{
  "expo": {
    "name": "Uber Clone",
    "slug": "uber-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ride_app_icon.png",
    "scheme": "ubercloneapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {},
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "server",
      "favicon": "./assets/images/ride_app_icon.png",
      "bundler": "metro"
    },
    "plugins": [
      ["expo-router", { "origin": "https://uber.com/" }],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ride_app_icon.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#fffddd",
          "dark": {
            "backgroundColor": "#000000456"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
